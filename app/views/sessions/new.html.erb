<!-- app/views/sessions/new.html.erb -->
<div  class="container_signup">
  <br>
  <div class="text-center"> <!-- Center-align the "Sign Up" text -->
    <h2>Log in</h2>
  </div>
  <br>
<%= form_with url: login_path, scope: :user do |form| %>
  <div class="mb-3">
    <%# <label for="rocket_Name" class="form-label">Email</label> %>
    <%= form.email_field :email, required: true, class: "form-control",placeholder:"Email" %>
  </div>
  <div>
  <div class="mb-3">
    <%# <label for="rocket_Name" class="form-label">Password</label> %>
    <%= form.password_field :password, required: true , class: "form-control",placeholder:"Password" %>
  </div>
  <div class="d-grid gap-2">
  <%= form.submit "Log in", class: "btn btn-primary" %>
  </div>
<% end %>
</div>